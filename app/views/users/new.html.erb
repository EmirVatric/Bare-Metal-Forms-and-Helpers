<%= form_for @user do |f|%>

<%= f.label :name%>
<%= f.text_field :name %>
<br />
<%= f.label :email%>
<%= f.email_field :email %>
<br />
<%= f.label :password%>
<%= f.password_field :password %>
<br />
<%= f.submit 'Submit'%>


<% end %>